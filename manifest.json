{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.alba.co.kr/job/Detail.asp?*",
                "http://www.alba.co.kr/job/Detail.asp?*"
            ],
            "js": ["parse.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}